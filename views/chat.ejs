    <title>Me-Chat V1</title>
  </head>
  <body>
    <div class="container-fluid">
      <div class="card" >
        <div class="header row card-panel  light-blue accent-4  ">
          <i class="fas fa-comment-alt"></i> ME CHAT
        </div>
      </div>

      <div class="box " style=" padding: 2%; margin-top: -1.4%; margin-bottom: -5.5%; padding-bottom:  55%;">

      <% for(var i=0; i<see.length; i++){ %>
        <div class="item ">
         
        <p class="yousend "> <%= see[i] %></p>
    </div>
   <%  } %>
   
   
